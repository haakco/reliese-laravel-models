{
  "name": "haakco/reliese-laravel-models",
  "description": "Fork of Reliese Components for Laravel Framework code generation. With patches for postgresql, and a couple other things.",
  "keywords": [
    "reliese",
    "laravel",
    "haakco",
    "model"
  ],
  "homepage": "http://haak.co",
  "support": {
    "issues": "https://github.com/haakco/reliese-laravel-models/issues",
    "source": "https://github.com/haakco/reliese-laravel-models"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Cristian Llanos",
      "email": "cristianllanos@outlook.com"
    },
    {
      "name": "Tim Haak",
      "email": "tim@haak.co"
    }
  ],
  "require": {
    "php": ">=7.3.0",
    "doctrine/dbal": "^2.10",
    "illuminate/support": "^6.12",
    "illuminate/database": "^6.12",
    "illuminate/contracts": "^6.12",
    "illuminate/filesystem": "^6.12",
    "illuminate/console": "^6.12"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "fzaninotto/faker": "^1.9",
    "mockery/mockery": "^1.3",
    "phpunit/phpunit": "^8.5"
  },
  "autoload": {
    "psr-4": {
      "Reliese\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Reliese\\Coders\\CodersServiceProvider"
      ]
    }
  }
}
